# jEdit only needs to load the plugin the first time the user accesses it
# the presence of this property also tells jEdit the plugin is using the new API
plugin.com.illcode.jedit.inputreplace.InputReplacePlugin.activate=defer
plugin.com.illcode.jedit.inputreplace.InputReplacePlugin.usePluginHome=true

# These two properties are required for all plugins
plugin.com.illcode.jedit.inputreplace.InputReplacePlugin.name=InputReplace
plugin.com.illcode.jedit.inputreplace.InputReplacePlugin.author=Jesse Pavel

# version number
plugin.com.illcode.jedit.inputreplace.InputReplacePlugin.version=1.0.0

# online help
plugin.com.illcode.jedit.inputreplace.InputReplacePlugin.docs=doc/InputReplace.html

# we only have one dependency, jEdit 5.2
plugin.com.illcode.jedit.inputreplace.InputReplacePlugin.depend.0=jedit 05.02.00.01
plugin.com.illcode.jedit.inputreplace.InputReplacePlugin.depend.1=jdk 1.7

plugin.com.illcode.jedit.inputreplace.InputReplacePlugin.description=\
    A plugin that provides for an input replacement.

